    <script>
        function printSection(category) {
            // Create a new window with only the selected category
            const sections = document.querySelectorAll(`.${category}`);
            if (sections.length === 0) {
                alert('No products found in this category');
                return;
            }
            
            const printWindow = window.open('', '_blank');
            const css = document.querySelector('style').outerHTML;
            
            let content = `
                <!DOCTYPE html>
                <html>
                <head>
                    <title>OHS ${category} Labels</title>
                    ${css}
                </head>
                <body style="padding: 20px;">
            `;
            
            sections.forEach(section => {
                const productSection = section.closest('.product-section');
                if (productSection) {
                    content += productSection.outerHTML;
                }
            });
            
            content += '</body></html>';
            
            printWindow.document.open();
            printWindow.document.write(content);
            printWindow.document.close();
            
            setTimeout(() => {
                printWindow.print();
            }, 500);
        }
    </script>
    

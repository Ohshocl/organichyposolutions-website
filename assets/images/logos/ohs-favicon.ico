<svg viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Primary Blue Gradient -->
    <linearGradient id="primaryBlue" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2E5EAA;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e3a8a;stop-opacity:1" />
    </linearGradient>
    
    <!-- Light Blue for Text -->
    <linearGradient id="lightBlue" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7DD3FC;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#bfdbfe;stop-opacity:1" />
    </linearGradient>
    
    <!-- Organic Green Accent -->
    <radialGradient id="organicAccent" cx="50%" cy="20%" r="40%">
      <stop offset="0%" style="stop-color:#4ADE80;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#4ADE80;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect x="0" y="0" width="32" height="32" fill="url(#primaryBlue)" rx="6" ry="6"/>
  
  <!-- Organic Accent Glow -->
  <rect x="0" y="0" width="32" height="32" fill="url(#organicAccent)" rx="6" ry="6"/>
  
  <!-- "O" Letter -->
  <text x="6" y="22" font-family="Arial Black, sans-serif" font-size="12" font-weight="900" 
        fill="url(#lightBlue)" text-anchor="middle">O</text>
  
  <!-- "H" Letter -->
  <text x="16" y="22" font-family="Arial Black, sans-serif" font-size="12" font-weight="900" 
        fill="url(#lightBlue)" text-anchor="middle">H</text>
  
  <!-- "S" Letter -->
  <text x="26" y="22" font-family="Arial Black, sans-serif" font-size="12" font-weight="900" 
        fill="url(#lightBlue)" text-anchor="middle">S</text>
  
  <!-- Small Organic Indicator -->
  <circle cx="28" cy="6" r="2" fill="#4ADE80"/>
  <circle cx="28" cy="6" r="1" fill="#ffffff"/>
</svg>

<svg viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Main FDA Blue Gradient -->
    <linearGradient id="fdaBlueGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e3a8a;stop-opacity:1" />
    </linearGradient>
    
    <!-- FDA Red Gradient -->
    <linearGradient id="fdaRedGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#dc2626;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#b91c1c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#991b1b;stop-opacity:1" />
    </linearGradient>
    
    <!-- Green Approval Gradient -->
    <linearGradient id="approvalGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#16a34a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#15803d;stop-opacity:1" />
    </linearGradient>
    
    <!-- Clean White Gradient -->
    <linearGradient id="cleanWhite" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8fafc;stop-opacity:1" />
    </linearGradient>
    
    <!-- Professional Shadow -->
    <filter id="professionalShadow">
      <feDropShadow dx="2" dy="3" stdDeviation="2" flood-color="rgba(0,0,0,0.15)"/>
    </filter>
    
    <!-- Food Safety Pattern -->
    <pattern id="foodPattern" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
      <circle cx="10" cy="10" r="1" fill="rgba(22,163,74,0.08)"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect x="0" y="0" width="400" height="300" fill="#ffffff" rx="15" ry="15"/>
  
  <!-- Professional Border -->
  <rect x="8" y="8" width="384" height="284" fill="none" stroke="url(#fdaBlueGradient)" stroke-width="3" rx="12" ry="12"/>
  <rect x="12" y="12" width="376" height="276" fill="none" stroke="url(#fdaRedGradient)" stroke-width="1" rx="10" ry="10"/>
  
  <!-- Header Section -->
  <rect x="20" y="20" width="360" height="70" fill="url(#fdaBlueGradient)" rx="8" ry="8"/>
  
  <!-- FDA Shield Symbol -->
  <g transform="translate(40,35)">
    <!-- Main Shield -->
    <path d="M25,5 Q35,5 35,15 L35,40 Q35,50 25,55 Q15,50 15,40 L15,15 Q15,5 25,5 Z" 
          fill="#ffffff" filter="url(#professionalShadow)"/>
    
    <!-- Inner Shield Details -->
    <rect x="18" y="12" width="14" height="10" fill="url(#fdaRedGradient)" rx="2"/>
    <text x="25" y="20" text-anchor="middle" font-family="Arial Black, sans-serif" 
          font-size="6" font-weight="900" fill="#ffffff">FDA</text>
    
    <!-- Food Safety Symbol -->
    <circle cx="25" cy="32" r="8" fill="url(#approvalGradient)"/>
    <path d="M21,32 L24,35 L29,30" stroke="#ffffff" stroke-width="2" fill="none" stroke-linecap="round"/>
    
    <!-- Shield Accent -->
    <path d="M20,45 Q25,48 30,45" stroke="url(#fdaBlueGradient)" stroke-width="1.5" fill="none"/>
  </g>
  
  <!-- Header Text -->
  <text x="85" y="42" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#ffffff">
    U.S. FOOD AND DRUG
  </text>
  <text x="85" y="58" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#7DD3FC">
    ADMINISTRATION
  </text>
  <text x="85" y="74" font-family="Arial, sans-serif" font-size="10" font-weight="500" fill="rgba(255,255,255,0.8)">
    Food Contact Substance Notification
  </text>
  
  <!-- Main Content Area -->
  <rect x="20" y="100" width="360" height="130" fill="url(#cleanWhite)" rx="8" ry="8"/>
  <rect x="20" y="100" width="360" height="130" fill="url(#foodPattern)" opacity="0.3"/>
  
  <!-- Approval Header -->
  <text x="200" y="125" text-anchor="middle" font-family="Arial, sans-serif" 
        font-size="18" font-weight="bold" fill="url(#fdaBlueGradient)">
    FDA APPROVED
  </text>
  <text x="200" y="142" text-anchor="middle" font-family="Arial, sans-serif" 
        font-size="14" font-weight="600" fill="url(#fdaRedGradient)">
    Food Contact Substance
  </text>
  
  <!-- Notification Number -->
  <g transform="translate(200,160)">
    <rect x="-75" y="-15" width="150" height="30" fill="url(#approvalGradient)" rx="15" ry="15"/>
    <text x="0" y="2" text-anchor="middle" font-family="Arial Black, sans-serif" 
          font-size="13" font-weight="900" fill="#ffffff">
      FCN 1811
    </text>
  </g>
  
  <!-- Safety Statement -->
  <text x="200" y="190" text-anchor="middle" font-family="Arial, sans-serif" 
        font-size="12" font-weight="600" fill="url(#fdaBlueGradient)">
    Safe for Direct Food Contact
  </text>
  
  <!-- No Rinse Required -->
  <g transform="translate(200,205)">
    <rect x="-65" y="-8" width="130" height="16" fill="rgba(22,163,74,0.1)" rx="8" ry="8"/>
    <text x="0" y="2" text-anchor="middle" font-family="Arial, sans-serif" 
          font-size="10" font-weight="600" fill="url(#approvalGradient)">
      No Rinse Required
    </text>
  </g>
  
  <!-- Footer Section -->
  <rect x="20" y="240" width="360" height="40" fill="#f1f5f9" rx="8" ry="8"/>
  
  <!-- Footer Content -->
  <text x="200" y="255" text-anchor="middle" font-family="Arial, sans-serif" 
        font-size="10" font-weight="600" fill="url(#fdaBlueGradient)">
    FOOD CONTACT SUBSTANCE NOTIFICATION
  </text>
  <text x="200" y="268" text-anchor="middle" font-family="Arial, sans-serif" 
        font-size="9" font-weight="400" fill="#64748b">
    21 CFR Part 170 - Food Additives | Effective Upon Notification
  </text>
  
  <!-- Approval Elements -->
  <g transform="translate(50,250)">
    <circle cx="0" cy="0" r="8" fill="url(#approvalGradient)"/>
    <path d="M-3,-1 L0,2 L3,-2" stroke="#ffffff" stroke-width="2" fill="none" stroke-linecap="round"/>
  </g>
  
  <g transform="translate(350,250)">
    <circle cx="0" cy="0" r="8" fill="url(#approvalGradient)"/>
    <path d="M-3,-1 L0,2 L3,-2" stroke="#ffffff" stroke-width="2" fill="none" stroke-linecap="round"/>
  </g>
  
  <!-- Professional Dividers -->
  <line x1="30" y1="95" x2="370" y2="95" stroke="url(#fdaRedGradient)" stroke-width="2"/>
  <line x1="30" y1="235" x2="370" y2="235" stroke="url(#approvalGradient)" stroke-width="1"/>
  
  <!-- Food Safety Icons -->
  <!-- Utensil Icon -->
  <g transform="translate(80,180)">
    <path d="M-5,-8 L-5,8 M-3,-8 L-3,-4 M-1,-8 L-1,-4 M1,-8 L1,8 M3,-6 Q5,-6 5,-2 L5,8" 
          stroke="url(#fdaBlueGradient)" stroke-width="1.5" fill="none" stroke-linecap="round"/>
  </g>
  
  <!-- Drop Icon -->
  <g transform="translate(320,180)">
    <path d="M0,-8 Q4,-4 4,0 Q4,4 0,8 Q-4,4 -4,0 Q-4,-4 0,-8 Z" 
          fill="url(#approvalGradient)" opacity="0.7"/>
  </g>
  
  <!-- Security Pattern -->
  <rect x="25" y="105" width="350" height="120" fill="none" stroke="rgba(30,64,175,0.05)" stroke-width="1" stroke-dasharray="3,3"/>
  
  <!-- OHS Branding (Subtle) -->
  <text x="385" y="295" text-anchor="end" font-family="Arial, sans-serif" 
        font-size="6" fill="rgba(30,64,175,0.3)">
    organichyposolutions.com
  </text>
</svg>

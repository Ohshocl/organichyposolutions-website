{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "organichyposolutions-static",
  "description": "Organic HypoSolutions - Static Site (API functions coming later)",
  "public": false,
  "trailingSlash": false,
  "cleanUrls": true,
  "github": {
    "enabled": false
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
